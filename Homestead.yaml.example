---
ip: "192.168.10.10"
memory: 2048
cpus: 1
hostname: desmitificador-pr
name: desmitificador-pr
provider: virtualbox

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    - map: "/Users/pala/workspace/desmitificador"
      to: "/home/vagrant/desmitificador"

sites:
    - map: desmitificador.dev
      to: "/home/vagrant/desmitificador/public"

databases:
    - homestead

variables:
    - key: APP_ENV
      value: local

